{
  "version": 2,
  "name": "banyuwangi-marketplace-integrator",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/products",
      "dest": "/api/products.js"
    },
    {
      "src": "/api/normalize/(.*)",
      "dest": "/api/normalize.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/api/products.js"
    }
  ],
  "functions": {
    "api/products.js": {
      "maxDuration": 30
    },
    "api/normalize.js": {
      "maxDuration": 30
    },
    "api/health.js": {
      "maxDuration": 30
    }
  }
}